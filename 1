bool process_record_user(uint16_t keycode, keyrecord_t *record) {
    
    switch (keycode) {
        case NC1:
            if (record->event.pressed) {
                // when keycode NC1 is pressed
                SEND_STRING("No Conflicts.");
            } else {
                // when keycode NC1 is released
            }
            break;
            
        case KVF:
            if (record->event.pressed) {
                SEND_STRING("Kassandra Vanessa Flores");
            } else {
                    }
                    break;
        case EMJ:
            if (record->event.pressed) {
                SEND_STRING("Heriberto Morales Jr.");
            } else {
                    }
                    break;
        case MH1:
            if (record->event.pressed) {
                SEND_STRING("Mireya");
            } else {
                    }
                    break;
        
        case MR1:
            if (record->event.pressed) {
                SEND_STRING("Moni");
            } else {
                    }
                    break;
        case NM1:
            if (record->event.pressed) {
                SEND_STRING("Norma");
            } else {
                    }
                    break;
        
        case BS1:
            if (record->event.pressed) {
                SEND_STRING("Blanca");
            } else {
                    }
                    break;
        
        case PW1:
            if (record->event.pressed) {
                SEND_STRING("y8tk(TQW");
            } else {
                    }
                    break;
       
        case BEST:
            if (record->event.pressed) {
                SEND_STRING("Best,");
            } else {
                    }
                    break;
            
            switch (keycode) {
                case EMAIL:
                    if (record->event.pressed) {
                        // when keycode NC1 is pressed
                        SEND_STRING("KFlores@LangleyBanack.com");
                    } else {
                        // when keycode NC1 is released
                    }
                    break;
                    
            switch (keycode) {
                case GMAIL:
                    if (record->event.pressed) {
                        // when keycode NC1 is pressed
                        SEND_STRING("KassandraVFlores@Gmail.com");
                        } else {
                        // when keycode NC1 is released
                            }
                    break;
                    
            switch (keycode) {
                case ST1:
                    if (record->event.pressed) {
                        // when keycode NC1 is pressed
                        SEND_STRING("Storm");
                        } else {
                        // when keycode NC1 is released
                                    }
                    break;
                
            switch (keycode) {
                case PW2:
                    if (record->event.pressed) {
                    SEND_STRING("L&B1");
                    } else {
                            }
                    break;
            }
      
            return true;
                    
            };


